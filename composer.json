{
    "name": "paybas/breadcrumbmenu",
    "type": "phpbb-extension",
    "description": "This extension adds a drop-down menu to each breadcrumb, containing the sibling and child forums.",
    "homepage": "https://github.com/sajaki/BreadcrumbMenu",
    "version": "1.0.6",
    "time": "2017-01-24",
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "PayBas",
            "email": "admin@pbwow.com",
            "homepage": "http://www.paybas.com",
            "role": "Lead Developer"
        },
        {
            "name": "Sajaki",
            "email": "Sajaki@avathar.be",
            "homepage": "http://www.avathar.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "composer/installers": "~1.0"
    },
    "require-dev": {
        "phpbb/epv": "dev-master"
    },
    "extra": {
        "display-name": "Breadcrumb Menu",
        "soft-require": {
            "phpbb/phpbb": ">=3.1.3,<3.2@dev"
        },
        "version-check": {
            "host": "www.avathar.be",
            "directory": "/versioncheck",
            "filename": "breadcrumbmenu.json"
        }
    }
}
